{
  "compilerOptions": {
  "target": "es5",
  "module": "commonjs",
  "rootDir": "./",
  "baseUrl": "./src",
  "outDir": "./dist",
  "paths": {
      "@config/*": ["config/*"],
      "@modules/*": ["modules/*"],
      "@shared/*": ["shared/*"]
  },
  "esModuleInterop": true,
  "experimentalDecorators": true,
  "emitDecoratorMetadata": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true,
  "resolveJsonModule": true
  }
}